CREATE TABLE public.tb_maps
(
  id integer NOT NULL DEFAULT nextval('tb_maps_id_seq'::regclass),
  title character varying(30) NOT NULL,
  name character varying(30) NOT NULL,
  description character varying NOT NULL,
  type character varying(20) NOT NULL,
  atribs character varying NOT NULL,
  rep_id character varying(50) NOT NULL,
  datestart date NOT NULL,
  status integer NOT NULL,
  CONSTRAINT tb_maps_pkey PRIMARY KEY (id)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE public.tb_maps
  OWNER TO postgres;

